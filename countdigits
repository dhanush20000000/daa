import java.util.Scanner;
class Solution {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Enter a number: ");
        int num = scanner.nextInt();
        scanner.close();
        
        int count = 0;
        
        while (num != 0) {
            num =num/ 10;
            count++;
        }
        
        System.out.println( count);
    }
}
